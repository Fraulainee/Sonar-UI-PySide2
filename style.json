{
	"QCustomSlideMenu": [{
		"name": "side_menu",
		"floatPosition": [{
				"relativeTo": "main_body",
				"position": "top-left",
				"shadow":[{
					"color": "#000000",
					"offset": {
						"x": 0,
						"y": 0
					},
					"blur": 20
				}]
		}],
		"defaultSize": [{
			"width": 150,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": 0
		}],
		"expandedSize": [{
			"width": 150,
			"height": "parent"
		}],
		"toggleButton": [{
			"buttonName": "menubtn"
		}],
		"menuTransitionAnimation": [{
			"duration": 1000,
			"easing": "outBack"
		}]
	}]
}
